<%= form_for @service do |f| %>
    <%= f.label :title %><br>
    <%= f.text_field :title %><br><br>
    <%= f.label :description %><br>
    <%= f.text_field :description %><br><br>
    <%= f.label :requirements %><br>
    <%= f.text_field :requirements %><br><br>
    <%= f.label :price %><br>
    <%= f.number_field :price, in:(1..10000) %><br><br>
    <%= f.submit %>
<% end %>
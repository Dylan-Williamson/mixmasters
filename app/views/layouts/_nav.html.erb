<% if logged_in? %>
<h3><%= link_to @current_user.username, @current_user %> | <%= link_to "Logout", sessions_path, method: :delete %></h3>
<% else %>
<h3>You're Not Logged In :( <%= link_to "Login", login_path %>, <%= link_to "Sign Up", new_user_path %></h3>
<%= link_to "Login with Google", "/auth/google_oauth2" %>
<% end %>